<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        <title>Listar Proveedores</title>
        <link rel="stylesheet" href="/stylesheets/indexlistar.css"/>
    </head>
    <body>

        <div>
          <table class="table table-bordered table-striped" id="TablaListar">
            <caption style="font-size: 50px;"><b>Lista de Proveedores</b></caption>
              <thead>
                <tr>
                  <th>Empresa</th>
                  <th>CIF</th>
                  <th>WEB</th>
                  <th>NumeroM</th>
                  <th>TipodeS</th>
                </tr>
              </thead>
              <tbody id="Datostabla">
              </tbody>
          </table>
        </div>
        <p id="Datos" class="invisible"> <%= JSON %> </p>

        <script>

          let res = document.getElementById("Datostabla")
          let Datosjson = JSON.parse(document.getElementById("Datos").innerText)

          res.innerHTML = ""
          for(let item of Datosjson){

            res.innerHTML += `
        
              <tr>
                <td> ${item.Empresa} </td>
                <td> ${item.CIF} </td>
                <td> ${item.WEB} </td>
                <td> ${item.NumeroM} </td>
                <td> ${item.TipodeS} </td>
              </tr>
            `
          }
        </script>
        <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
        <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    </body>
</html>